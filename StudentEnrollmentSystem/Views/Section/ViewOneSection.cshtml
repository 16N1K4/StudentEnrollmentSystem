@model Section

<h3 align="center">Section Details</h3>
<a class="btn btn-info" asp-controller="Section" asp-action="ViewAllSections" class="btn btn-info"><i class="bi bi-box-arrow-left"></i> Back to list</a>
<br />
<table class="table table-striped table-info">
    <tbody>
        <tr>
            <th scope="col">ID</th>
            <td>@Model.ID</td>

        </tr>
        <tr>
            <th scope="col">SectionCode</th>
            <td>@Model.SectionCode</td>

        </tr>
        <tr>
            <th scope="col">Day</th>
            <td>@Model.Day</td>
        </tr>
        <tr>
            <th scope="col">Start Time</th>
            <td>@Model.StartTime</td>
        </tr>
        <tr>
            <th scope="col">End Time</th>
            <td>@Model.EndTime</td>
        </tr>
    </tbody>
</table>
