@model Faculty

<h3 align ="center">Faculty Details</h3>
<a class="btn btn-info" asp-controller="Faculty" asp-action="ViewAllFaculty" class="btn btn-info"><i class="bi bi-box-arrow-left"></i> Back to list</a>
<br />
<table class="table table-striped table-info">
    <tbody>
        <tr>
            <th scope="col">ID</th>
            <td>@Model.ID</td>

        </tr>
        <tr>
            <th scope="col">Name</th>
            <td>@Model.FirstName @Model.LastName</td>
        </tr>
        <tr>
            <th scope="col">Email Address</th>
            <td>@Model.Email</td>
        </tr>
    </tbody>
</table>
