@model Department

<h3 align ="center">Department Details</h3>
<a class="btn btn-info" asp-controller="Department" asp-action="ViewAllDepartments" class="card-link"><i class="bi bi-box-arrow-left"></i> Back to list</a>
<br />
<table class="table table-striped table-info">
    <tbody>
        <tr>
            <th scope="col">ID</th>
            <td>@Model.ID</td>

        </tr>
        <tr>
            <th scope="col">Name</th>
            <td>@Model.Name</td>

        </tr>
    </tbody>
</table>